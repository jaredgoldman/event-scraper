{
  "name": "eventScraper",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "build": "pnpm i && pnpm run db:generate && tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:push": "prisma db push",
    "db:reset": "dotenv -e .env -- prisma migrate reset --force --skip-seed && pnpm run db:push && pnpm run db:seed",
    "db:seed": "ts-node src/scripts/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/jsdom": "^21.1.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.2",
    "@types/node-cron": "^3.0.11",
    "envalid": "^8.0.0",
    "nodemon": "^3.1.3",
    "prisma": "^5.15.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@langchain/cohere": "^0.1.0",
    "@langchain/community": "^0.2.11",
    "@langchain/core": "^0.2.6",
    "@langchain/groq": "^0.0.12",
    "@langchain/mistralai": "^0.0.24",
    "@langchain/openai": "^0.1.3",
    "@langchain/textsplitters": "^0.0.3",
    "@prisma/client": "^5.15.0",
    "cheerio": "1.0.0-rc.12",
    "cheminfo-types": "^1.7.3",
    "dotenv-cli": "^7.4.2",
    "html-to-text": "^9.0.5",
    "jsdom": "^24.1.0",
    "langchain": "^0.2.5",
    "luxon": "^3.4.4",
    "node-cron": "^3.0.3",
    "openai": "^4.51.0",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "puppeteer": "^22.11.0",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
  }
}
